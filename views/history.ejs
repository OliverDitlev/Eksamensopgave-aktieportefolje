<body>
    <h1>Portfolio History</h1>
   
</body>
    <div class="history-container">
        <table class="history-table">
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Action</th>
                    <th>Stock</th>
                    <th>Price</th>
                    <th>Quantity</th>
                </tr>
            </thead>
            <tbody>
                <% history.forEach(item => { %>
                    <tr>
                        <td><%= item.date %></td>
                        <td><%= item.action %></td>
                        <td><%= item.stock %></td>
                        <td><%= item.price %></td>
                        <td><%= item.quantity %></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>